//Name			:	9.c
//Descriptiom	:	求3X3非对角线数之和
//Version		:	1.0
//Creator		:	haha 2019.02.25
//××××××××××××××××××××××******************************//
#include <stdio.h>
int main()
{
	int num[3][3]={1,2,3,4,5,6,7,8,9};
	int i,j,sum=0;
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
		printf("%d ",num[i][j]);
		}
		printf("\n");
	}
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			if((i+j ==3)||(i==j))
			{
			}
			else
			{
			sum += num[i][j];
			}
		}
	}
	printf("%d\n",sum);
	return 0;
}
