//Name			:	8.c
//Descriptiom	:	输入一串数字，逆序存储并输出
//Version		:	1.0
//Creator		:	haha 2019.02.25
//××××××××××××××××××××××******************************//
#include <stdio.h>
#include <string.h>
int main()
{
	int num[5]={1,2,3,4,5};
	int i,n;
	for(i=0;i<5/2;i++)
	{
	n=num[i];
	num[i]=num[5-1-i];
	num[5-1-i]=n;
	}
	for(i=0;i<5;i++)
	{
	printf("%d",num[i]);
	}	
	return 0;
}
